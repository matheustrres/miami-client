{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "target": "es2021",
    "module": "commonjs",
    "baseUrl": ".",
    "outDir": "./dist",
    "resolveJsonModule": true,
    "allowJs": true,
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "paths": {
      "@config": [
        "./src/config"
      ],
      "@commands/*": [
        "./src/commands/*"
      ],
      "@events/*": [
        "./src/events/*"
      ],
      "@structs/*": [
        "./src/structs/*"
      ],
      "@typings/*": [
        "./src/typings/*"
      ]
    },
    "rootDirs": [
      "./src",
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./src/typings"
    ],
    "emitDecoratorMetadata": true,
    "noFallthroughCasesInSwitch": false,
    "strict": true,
    "strictPropertyInitialization": false,
  },
}