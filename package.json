{
  "name": "miami-client",
  "author": {
    "name": "Matheus Torres",
    "email": "matheustrres.rj@gmail.com"
  },
  "description": "A discord client built with typescript to ease everyday activities and make them more enjoyable",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.16.1",
    "npm": "9.5.1"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc --build ./tsconfig.build.json",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "start": "dotenv -e .env -- node --expose-gc .",
    "start:dev": "dotenv -e .env.local -- tsx watch ./src/index.ts --tsconfig ./tsconfig.json --ignore node_modules",
    "prepare": "husky install"
  },
  "dependencies": {
    "discord.js": "^14.11.0",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.44.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "node.js",
    "discord.js",
    "client"
  ]
}
